{
    "metadata": [{
        "src": [{
            "files": [
                "**.fsproj"
            ],
            "src": "path\\to\\project"
        }],
        "dest": "api",
        "disableGitFeatures": false,
        "disableDefaultFilter": false
    }],
    "build": {
        "content": [{
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [{
            "files": [
                "images/**"
            ]
        }],
        "overwrite": [{
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "docs/**"
            ]
        }],
        "dest": "docs",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/darkfx/darkfx"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}